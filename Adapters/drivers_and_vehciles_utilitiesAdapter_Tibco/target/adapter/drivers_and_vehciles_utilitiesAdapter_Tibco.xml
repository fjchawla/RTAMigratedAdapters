<mfp:adapter name="drivers_and_vehciles_utilitiesAdapter_Tibco" xmlns:http="http://www.ibm.com/mfp/integration/http" xmlns:mfp="http://www.ibm.com/mfp/integration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <displayName>drivers_and_vehciles_utilitiesAdapter_Tibco</displayName>
    <description>drivers_and_vehciles_utilitiesAdapter_Tibco</description>
    <connectivity>
        <connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
            <protocol>https</protocol>
            <domain>${localhost}$</domain>
            <port>${localhostport}$</port>
            <connectionTimeoutInMilliseconds>200000</connectionTimeoutInMilliseconds>
            <socketTimeoutInMilliseconds>200000</socketTimeoutInMilliseconds>
            <maxConcurrentConnectionsPerNode>100</maxConcurrentConnectionsPerNode>
        </connectionPolicy>
    </connectivity>
    <procedure name="buildBody" />
    <procedure name="buildBodyFromStaticRequest" />
    <procedure name="getApplicationSettings" />
    <procedure name="getTibcoMode" />
    <procedure name="getChannelCredentials" />
    <procedure name="getExternalChannelCredentials" />
    <procedure name="getTibcoCredentials" />
    <procedure name="sendMail" />
    <procedure name="encryptData" />
    <procedure name="decryptData" />
    <procedure name="deleteCredientails" />
    <procedure name="generatePassword" />
    <procedure name="invokeEncryptedProcedure" />
    <procedure name="getEtrafficDonationServices" />
    <procedure name="getDonationServices" />
    <procedure name="getDonationServices_iOS" />

    <property name="drivers_and_vehicles_is_debugging"  description="drivers_and_vehicles_is_debugging" defaultValue="${DriversAndVehiclesIsDebugging}$"/>
    <property name="wsse.tibco.username"  description="wsse.tibco.username" defaultValue="${wsseTibcoUsername}$"/>
    <property name="wsse.tibco.password"  description="wsse.tibco.password" defaultValue="${wsseTibcoPassword}$"/>
	<property name="drivers_and_vehicles_mail_port"  description="drivers_and_vehicles_mail_port" defaultValue="${driversandVehiclesMailPort}$"/>
	<property name="drivers_and_vehicles_mail_host"  description="drivers_and_vehicles_mail_host" defaultValue="${driversandVehiclesMailHost}$"/>
	<property name="drivers_and_vehicles_mail_toMailAddress"  description="drivers_and_vehicles_mail_toMailAddress" defaultValue="driversandVehiclesMailtoMailAddress"/>
	<property name="drivers_and_vehicles_mail_user"  description="drivers_and_vehicles_mail_user" defaultValue="${driversandVehiclesMailUser}$"/>
	<property name="drivers_and_vehicles_mail_pass"  description="drivers_and_vehicles_mail_pass" defaultValue="driversandVehiclesMailPass"/>
</mfp:adapter>
