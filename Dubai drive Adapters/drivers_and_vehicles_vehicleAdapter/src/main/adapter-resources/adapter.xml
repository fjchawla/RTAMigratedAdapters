<mfp:adapter name="drivers_and_vehicles_vehicleAdapter" xmlns:http="http://www.ibm.com/mfp/integration/http" xmlns:mfp="http://www.ibm.com/mfp/integration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <displayName>drivers_and_vehicles_vehicleAdapter</displayName>
    <description>drivers_and_vehicles_vehicleAdapter</description>
    <connectivity>
        <connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
            <protocol>https</protocol>
            <domain>test12c</domain>
            <port>7783</port>
            <connectionTimeoutInMilliseconds>30000</connectionTimeoutInMilliseconds>
            <socketTimeoutInMilliseconds>30000</socketTimeoutInMilliseconds>
            <maxConcurrentConnectionsPerNode>100</maxConcurrentConnectionsPerNode>
        </connectionPolicy>
    </connectivity>
    <procedure name="bookletLicenseInquiryService" />
    <procedure name="mCardService" />
    <procedure name="ownerVehicleInfoService" />
    <procedure name="trafficInquiryService" />
    <procedure name="userOwnedAndBookedPlatesService" />
    <procedure name="usersVehiclesService" />
    <procedure name="reInsuranceCertificateService" />
    <procedure name="returenVehicleFromTourService" />
    <procedure name="getVehicleInformationAmendment" />

    <property name="drivers_and_vehicles_is_debugging"  description="drivers_and_vehicles_is_debugging" defaultValue="true"/>
</mfp:adapter>
